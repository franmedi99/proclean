                                                    <div class="card-header">
                                                        <h1>Buscar Cliente</h1>
                                                    </div>
                                                    <div class="card-body">
                                                        <div class="ml-3 mr-3">
                                                            <form action="/clients/find-client" method="post">
                                                                <div class="form-group mb-3">
                                                                    <label for="tpatente"
                                                                        class="text-left">Patente</label>
                                                                    <input list="tpatente"
                                                                        class="form-control text-center" name="patente"
                                                                        autocomplete="off" />
                                                                    <datalist id="tpatente">
                                                                        {{#each clients}}
                                                                        <option value="{{patente}}">
                                                                            {{/each}}
                                                                    </datalist>
                                                                </div>
                                                                <div class="form-group">
                                                                    <button type="submit" id="entrar2"
                                                                        class="btn btn-success mx-auto btn-lg btn-block"><i
                                                                            class="fa fa-search mr-1"
                                                                            aria-hidden="true"></i>Buscar</button>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="mb-3 mx-auto">
                                <button type="button" class="btn btn-secondary mx-auto btn-lg" data-dismiss="modal"><i
