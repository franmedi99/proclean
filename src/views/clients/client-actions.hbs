                data-target=".bd-nuevo-modal-lg"><i class="fa fa-user-circle-o fa-3x"></i>
                <h3>Nuevo Cliente</h3> </i>
            </a>
        </div>

        <div>
            <div class="modal fade bd-nuevo-modal-lg" aria-labelledby="myLargeModalLabel" tabindex="-1" role="dialog"
                id="ventana2">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content mx-auto mt-5">
                        <!-- Formulario registro-->

                        <div class="row">
                            <div class="col-md-12 mx-auto text-center">
                                <div class="card">
                                    <div class="card-header">
                                        <h1>Registrar Cliente</h1>
                                    </div>
                                    <div class="card-body">
                                        <form action="/clients/new-client" method="POST">
                                            <div class="form-group text-left">
                                                <label for="patente">Patente</label>
                                                <input type="text" name="patente" class="form-control text-center"
                                                    placeholder="Patente" value="{{client}}"
                                                    onkeyup="javascript:this.value=this.value.toUpperCase();"
                                                    autocomplete="off">
                                            </div>
                                            <div class="form-group  text-left">
                                                <label for="patente">Marca</label>
                                                <input type="text" name="marca" class="form-control text-center"
                                                    placeholder="Marca" value="{{username}}">
                                            </div>

                                            <div class="form-group  text-left">
                                                <label for="patente">Modelo</label>
                                                <input type="text" name="modelo" class="form-control text-center"
                                                    placeholder="Modelo" value="{{email}}">
                                            </div>

                                            <div class="form-group  text-left">
                                                <label for="patente">Telefono</label>
                                                <input type="tel" name="phone" class="form-control text-center"
                                                    placeholder="Telefono" value="{{phone}}" autocomplete="off">
                                            </div>
                                            <div class="form-group text-left">
                                                <label for="patente">Tipo de cuenta</label>
                                                <select class="form-control text-center mx-auto" name="cuenta">
                                                    <option value="comun">Comun</option>
                                                    <option value="corriente">Corriente</option>
                                                </select>
                                            </div>

                                            <div class="form-group text-left">
                                                <label for="patente">Tipo de vehiculo</label>
                                                <select class="form-control text-center mx-auto" name="tipo">
                                                    <option value="Auto_chico">Auto chico</option>
                                                    <option value="Auto_grande">Auto grande</option>
                                                    <option value="Camioneta">Camioneta</option>
                                                </select>
                                            </div>
                                            <div class="form-group">
                                                <button type="submit" class="btn btn-success btn-block btn-lg"><i
                                                        class="fa fa-plus mr-1" aria-hidden="true"></i>Registrar
                                                    Cliente</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary mx-auto btn-lg" data-dismiss="modal"><i
                                    class="fa fa-times mr-1" aria-hidden="true"></i>Cancelar</button>
                        </div>
                    </div><!-- /.modal-content -->
                </div><!-- /.modal-dialog -->
            </div><!-- /.modal -->
        </div>


