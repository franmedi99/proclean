<div>

    <div class="mx-auto">
    <h3 class="text-center"><i class="fa fa-user-o mr-1" aria-hidden="true"></i>Registro de Clientes</h3>
    </div>

    <table class="table col-11 mx-auto mt-3">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Patente</th>
            <th scope="col">Marca</th>
            <th scope="col">Modelo</th>
            <th scope="col">Tipo de Vehiculo</th>
            <th scope="col">Telefono</th>
            <th scope="col">Editar</th>
            <th scope="col">Borrar</th>
          </tr>

         
        </thead>
        <tbody>

         {{#each clients}}

          <tr>
          <th scope="row">{{patente}}</th>
          <th scope="row">{{marca}}</th>
          <th scope="row">{{modelo}}</th>
          <th scope="row">{{tipo}}</th>
          <th scope="row">{{phone}}</th>
          <td><a class="btn btn-primary col-12 mx-auto" href="/historial/edit/{{_id}}">Editar</a></td>
          <td>
           <form action="/historial/delete/{{_id}}" method="POST" onsubmit="return confirm('Desea eliminar este recibo de la caja?');">
           <button type="submit" class="btn btn-danger col-12 mx-auto">Borrar</button>
           </form>
           </td>
          </td>
          </tr>

          {{/each}}
          
        </tbody>
      </table>










</div>


